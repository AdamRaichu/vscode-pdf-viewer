{
  "name": "pdf-viewer",
  "displayName": "PDF Viewer",
  "description": "An extension which allows for viewing PDF files directly in VS Code Web using the browser native viewer.",
  "publisher": "adamraichu",
  "icon": "pdf-flat.png",
  "author": {
    "name": "AdamRaichu"
  },
  "categories": [
    "Visualization",
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/AdamRaichu/vscode-pdf-viewer"
  },
  "version": "1.0.0",
  "browser": "/dist/main.js",
  "engines": {
    "vscode": "^1.63.0"
  },
  "activationEvents": [
    "onCustomEditor:pdfViewer.PDFEdit"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "pdfViewer.PDFEdit",
        "displayName": "PDF Viewer",
        "selector": [
          {
            "filenamePattern": "*.pdf"
          }
        ]
      }
    ]
  },
  "scripts": {
    "build": "webpack"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}